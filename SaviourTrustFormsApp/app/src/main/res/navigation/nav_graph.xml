<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/Homepage">

    <fragment
        android:id="@+id/Homepage"
        android:name="com.saviourtrust.trustforms.Homepage"
        android:label="@string/homepage_label"
        tools:layout="@layout/fragment_homepage">

        <action
            android:id="@+id/action_Homepage_to_WeeklyForm"
            app:destination="@id/WeeklyForm" />

        <action
            android:id="@+id/action_Homepage_to_PropertyForm"
            app:destination="@id/PropertyForm" />
        <action
            android:id="@+id/action_Homepage_to_ServiceUserForm"
            app:destination="@id/ServiceUserForm" />
        <action
            android:id="@+id/action_Homepage_to_InitialAssessment"
            app:destination="@id/InitialAssessment" />

    </fragment>

    <fragment
        android:id="@+id/WeeklyForm"
        android:name="com.saviourtrust.trustforms.WeeklyForm"
        android:label="@string/weekly_form_label"
        tools:layout="@layout/fragment_weekly_form">

        <action
            android:id="@+id/action_WeeklyForm_to_Homepage"
            app:destination="@id/Homepage" />
    </fragment>

    <fragment
        android:id="@+id/PropertyForm"
        android:name="com.saviourtrust.trustforms.PropertyForm"
        android:label="@string/weekly_form_label"
        tools:layout="@layout/fragment_property_form">

        <action
            android:id="@+id/action_PropertyForm_to_Homepage"
            app:destination="@id/Homepage" />
    </fragment>

    <fragment
        android:id="@+id/ServiceUserForm"
        android:name="com.saviourtrust.trustforms.ServiceUserForm"
        android:label="Service User Form"
        android:layout="@layout/fragment_service_user_form" >
        <action
            android:id="@+id/action_ServiceUserForm_to_Homepage"
            app:destination="@id/Homepage" />
    </fragment>

    <fragment
        android:id="@+id/InitialAssessment"
        android:name="com.saviourtrust.trustforms.InitialAssessment"
        android:label="Initial Assessment" >
        <action
            android:id="@+id/action_InitialAssessment_to_InitialAssessment2"
            app:destination="@id/InitialAssessment2" />
        <action
            android:id="@+id/action_InitialAssessment_to_Homepage"
            app:destination="@id/Homepage" />
    </fragment>

    <fragment
        android:id="@+id/InitialAssessment2"
        android:name="com.saviourtrust.trustforms.InitialAssessment2"
        android:label="Initial Assessment Page 2" >
        <action
            android:id="@+id/action_InitialAssessment2_to_InitialAssessment3"
            app:destination="@id/initialAssessment3" />
        <action
            android:id="@+id/action_InitialAssessment2_to_InitialAssessment"
            app:destination="@id/InitialAssessment" />
    </fragment>

    <fragment
        android:id="@+id/initialAssessment3"
        android:name="com.saviourtrust.trustforms.InitialAssessment3"
        android:label="Initial Assessment Page 3"
        tools:layout="@layout/fragment_initial_assessment3" >
        <action
            android:id="@+id/action_InitialAssessment3_to_InitialAssessment4"
            app:destination="@id/InitialAssessment4" />
        <action
            android:id="@+id/action_InitialAssessment3_to_InitialAssessment2"
            app:destination="@id/InitialAssessment2" />
    </fragment>

    <fragment
        android:id="@+id/InitialAssessment4"
        android:name="com.saviourtrust.trustforms.InitialAssessment4"
        android:label="Initial Assessment Page 4"
        tools:layout="@layout/fragment_initial_assessment4" >
        <action
            android:id="@+id/action_InitialAssessment4_to_InitialAssessment5"
            app:destination="@id/InitialAssessment5" />
        <action
            android:id="@+id/action_InitialAssessment4_to_InitialAssessment3"
            app:destination="@id/initialAssessment3" />
    </fragment>

    <fragment
        android:id="@+id/InitialAssessment5"
        android:name="com.saviourtrust.trustforms.InitialAssessment5"
        android:label="Initial Assessment Page 5"
        tools:layout="@layout/fragment_initial_assessment5" >
        <action
            android:id="@+id/action_InitialAssessment5_to_InitialAssessment6"
            app:destination="@id/InitialAssessment6" />
        <action
            android:id="@+id/action_InitialAssessment5_to_InitialAssessment4"
            app:destination="@id/InitialAssessment4" />
    </fragment>

    <fragment
        android:id="@+id/InitialAssessment6"
        android:name="com.saviourtrust.trustforms.InitialAssessment6"
        android:label="Initial Assessment Page 6"
        tools:layout="@layout/fragment_initial_assessment6" >
        <action
            android:id="@+id/action_InitialAssessment6_to_InitialAssessment7"
            app:destination="@id/InitialAssessment7" />
        <action
            android:id="@+id/action_InitialAssessment6_to_InitialAssessment5"
            app:destination="@id/InitialAssessment5" />
    </fragment>

    <fragment
        android:id="@+id/InitialAssessment7"
        android:name="com.saviourtrust.trustforms.InitialAssessment7"
        android:label="Initial Assessment Page 7"
        tools:layout="@layout/fragment_initial_assessment7" >
        <action
            android:id="@+id/action_InitialAssessment7_to_Homepage"
            app:destination="@id/Homepage" />
        <action
            android:id="@+id/action_InitialAssessment7_to_InitialAssessment6"
            app:destination="@id/InitialAssessment6" />
    </fragment>

</navigation>