<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/Homepage">

    <fragment
        android:id="@+id/Homepage"
        android:name="com.saviourtrust.trustforms.Homepage"
        android:label="@string/homepage_label"
        tools:layout="@layout/fragment_homepage">

        <action
            android:id="@+id/action_Homepage_to_WeeklyForm"
            app:destination="@id/WeeklyForm" />

        <action
            android:id="@+id/action_Homepage_to_PropertyForm"
            app:destination="@id/PropertyForm" />
        <action
            android:id="@+id/action_Homepage_to_initialAssessment"
            app:destination="@id/initialAssessment" />
        <action
            android:id="@+id/action_Homepage_to_serviceUserForm"
            app:destination="@id/serviceUserForm" />

    </fragment>

    <fragment
        android:id="@+id/WeeklyForm"
        android:name="com.saviourtrust.trustforms.WeeklyForm"
        android:label="@string/weekly_form_label"
        tools:layout="@layout/fragment_weekly_form">

        <action
            android:id="@+id/action_WeeklyForm_to_Homepage"
            app:destination="@id/Homepage" />
    </fragment>

    <fragment
        android:id="@+id/PropertyForm"
        android:name="com.saviourtrust.trustforms.PropertyForm"
        android:label="@string/weekly_form_label"
        tools:layout="@layout/fragment_property_form">

        <action
            android:id="@+id/action_WeeklyForm_to_Homepage"
            app:destination="@id/Homepage" />
    </fragment>
    <fragment
        android:id="@+id/initialAssessment"
        android:name="com.saviourtrust.trustforms.InitialAssessment"
        android:label="InitialAssessment" >
        <action
            android:id="@+id/action_initialAssessment_to_initialAssessment2"
            app:destination="@id/initialAssessment2" />
    </fragment>
    <fragment
        android:id="@+id/initialAssessment2"
        android:name="com.saviourtrust.trustforms.InitialAssessment2"
        android:label="InitialAssessment2" >
        <action
            android:id="@+id/action_initialAssessment2_to_initialAssessment3"
            app:destination="@id/initialAssessment3" />
    </fragment>
    <fragment
        android:id="@+id/initialAssessment3"
        android:name="com.saviourtrust.trustforms.InitialAssessment3"
        android:label="fragment_initial_asessment3"
        tools:layout="@layout/fragment_initial_assessment3" >
        <action
            android:id="@+id/action_initialAssessment3_to_initialAssessment4"
            app:destination="@id/initialAssessment4" />
    </fragment>
    <fragment
        android:id="@+id/initialAssessment4"
        android:name="com.saviourtrust.trustforms.InitialAssessment4"
        android:label="fragment_initial_asessment4"
        tools:layout="@layout/fragment_initial_assessment4" >
        <action
            android:id="@+id/action_initialAssessment4_to_initialAssessment5"
            app:destination="@id/initialAssessment5" />
    </fragment>
    <fragment
        android:id="@+id/initialAssessment5"
        android:name="com.saviourtrust.trustforms.InitialAssessment5"
        android:label="fragment_initial_asessment5"
        tools:layout="@layout/fragment_initial_assessment5" >
        <action
            android:id="@+id/action_initialAssessment5_to_Homepage"
            app:destination="@id/Homepage" />
    </fragment>
    <fragment
        android:id="@+id/serviceUserForm"
        android:name="com.saviourtrust.trustforms.ServiceUserForm"
        android:label="ServiceUserForm" />

</navigation>